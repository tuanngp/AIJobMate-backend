# API Settings
API_VERSION=v1
PROJECT_NAME="AI JobMate - Career Advisor Service"

# PostgreSQL
POSTGRES_HOST=db
POSTGRES_USER=jobmate
POSTGRES_PASSWORD=secret
POSTGRES_DB=jobmate_db

# Redis
REDIS_HOST=redis
REDIS_PORT=6379

# Milvus
MILVUS_HOST=milvus
MILVUS_PORT=19530

# AWS
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=us-east-1
AWS_S3_BUCKET=ai-jobmate-cv-storage

# OpenAI
OPENAI_API_KEY=your_openai_api_key
CV_ANALYSIS_MODEL=gpt-4
CAREER_ADVICE_MODEL=gpt-4
EMBEDDING_MODEL=text-embedding-ada-002

# Security
JWT_SECRET_KEY=your-secret-key-at-least-32-characters
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=11520  # 8 days

# CORS
BACKEND_CORS_ORIGINS=["http://localhost:3000","http://localhost:8000"]

# Cache
CACHE_EXPIRE_TIME=3600  # 1 hour
